<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pai.knowledge.main">

    <application
        android:name="debug.MainApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/title_module_main"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".view.ui.ViewActivity"
            android:label="@string/title_module_main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.ui.MultiThreadActivity" />
        <activity android:name=".view.ui.AnimationActivity" />
        <activity
            android:name=".view.ui.MainActivity"
            android:label="@string/title_activity_main"></activity>
    </application>

</manifest>